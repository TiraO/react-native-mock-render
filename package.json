{
  "name": "react-native-mock-render-fork",
  "version": "0.0.7",
  "description": "A fork of react-native-mock-render",
  "main": "build/react-native.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha --require babel-core/register 'test/**/*.js'",
    "mocha:watch": "npm run test -- --watch",
    "build": "babel src --out-dir build",
    "lint": "eslint 'src/' 'test/' 'mock.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiraO/react-native-mock-render.git"
  },
  "keywords": [
    "react",
    "react-native",
    "mock",
    "testing"
  ],
  "author": "Root Insurance Co. Dev Team <devs@joinroot.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TiraO/react-native-mock-render/issues"
  },
  "homepage": "https://github.com/TiraO/react-native-mock-render",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-preset-airbnb": "2.2.3",
    "babel-preset-react-native": "1.9.1",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-react-native": "2.3.2",
    "mocha": "3.3.0",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0"
  },
  "dependencies": {
    "cubic-bezier": "^0.1.2",
    "invariant": "^2.2.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.15.0",
    "raf": "^3.2.0",
    "react-addons-perf": "^15.4.0",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-timer-mixin": "^0.13.3",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
